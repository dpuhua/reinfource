{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [
            false,
            "always"
        ],
        "quotemark":[
            true, 
            "single",
            "avoid-escape"
        ],
        "member-access": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "no-console": false,
        "trailing-comma": [
            true,
            { 
                "multiline": {
                    "objects": "never",
                    "arrays": "never",
                    "functions": "never",
                    "typeLiterals": "ignore"
                },
                "esSpecCompliant": true
            }
        ]
    },
    "rulesDirectory": []
}
